    "clean": "rimraf dist/* && rimraf bin/*",
    "lint": "eslint --ext .js --ext .jsx src && stylint src && npm run mdlint",
    "mdlint": "node tools/mdlint.js",
    "start-dev": "NODE_ENV=development node servers/dev.js",
    "windows-start-dev": "SET NODE_ENV=development & node servers/dev.js",
    "start-prod": "node bin/server.js",
    "build-prod": "npm run clean && npm run prod-client && npm run prod-server",
    "prod-client": "NODE_ENV=production webpack --config client.webpack.config.js",
    "prod-server": "NODE_ENV=production webpack --config server.webpack.config.js",
